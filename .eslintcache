Ref.child(`${user.uid}/colors`)\r\n                .push()\r\n                .set({\r\n                    primaryColor,\r\n                    secondaryColor\r\n                })\r\n                .then(()=>{\r\n                    setOpen(false)\r\n                })\r\n    }\r\n    return (\r\n        <SideColor >\r\n            <SideColor.Add onClick={()=>setOpen(true)}/>\r\n            {open && <SideColor.Model closeModel={()=>setOpen(false)}\r\n            handlerPrimaryColor={handlerPrimaryColor}\r\n            primaryColor={primaryColor}\r\n            handlerSecondaryColor={handlerSecondaryColor}\r\n            secondaryColor={secondaryColor}\r\n            addColors={addColorsHandler}\r\n            />}\r\n            {colors.length>0 && colors.map((color,i)=>(\r\n                <SideColor.Color key={i} color={color}\r\n                                onClick={()=>setColorRedux(color.primaryColor,color.secondaryColor)}\r\n                />\r\n            ))}\r\n        </SideColor>\r\n    )\r\n}\r\n\r\nexport default connect(null,{setColorRedux})(SideColorContainer)\r\n","C:\\Users\\ghizl\\OneDrive\\Desktop\\mini app\\react\\slack-chat-app\\src\\redux\\colors\\colorsReducer.js",[],"C:\\Users\\ghizl\\OneDrive\\Desktop\\mini app\\react\\slack-chat-app\\src\\redux\\colors\\colorsTypes.js",[],"C:\\Users\\ghizl\\OneDrive\\Desktop\\mini app\\react\\slack-chat-app\\src\\redux\\colors\\colorsActions.js",[],{"ruleId":"257","replacedBy":"258"},{"ruleId":"259","replacedBy":"260"},{"ruleId":"261","severity":1,"message":"262","line":25,"column":4,"nodeType":"263","endLine":25,"endColumn":6,"suggestions":"264"},{"ruleId":"261","severity":1,"message":"265","line":73,"column":8,"nodeType":"263","endLine":73,"endColumn":24,"suggestions":"266"},{"ruleId":"267","severity":1,"message":"268","line":100,"column":36,"nodeType":"269","endLine":100,"endColumn":112},{"ruleId":"261","severity":1,"message":"270","line":40,"column":8,"nodeType":"263","endLine":40,"endColumn":10,"